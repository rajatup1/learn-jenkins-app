{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "learn-jenkins-prod-taskdefinition",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "nginx:1.27-alpine",
            "portMappings": [
                {
                    "name": "nginx-tcp-80",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                    "appProtocol": "http"
                }
            ],
            
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": ""
}